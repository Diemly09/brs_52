<% if @target.class.table_name == "comments" %>
  $("#comment-<%= @target.id %>").find(".comment-like-<%= @target.id %>").
    html("<%= j render 'likes/dislikes', like: @like, target: @target %>")
<% elsif @target.class.table_name == "reviews" %>
  $(".review-<%= @target.id %>").find(".review-like-<%= @target.id %>").
    html("<%= j render 'likes/dislikes', like: @like, target: @target %>")
<% end %>
